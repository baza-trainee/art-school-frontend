    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-horizontal-scrolling-menu": "^4.1.1",
    "react-icons": "^4.12.0",
    "react-intersection-observer": "^9.5.3",
    "react-leaflet": "^4.2.1",
    "react-responsive": "^9.0.2",
